{
  "name": "{{PROJECT_NAME}}",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "A Runtime Accelerated Rendering Infrastructure (Rari) application",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "{{PACKAGE_MANAGER}} run clean && {{PACKAGE_MANAGER}} run typecheck && vite build",
    "dev": "vite",
    "predev": "{{PACKAGE_MANAGER}} run build",
    "start": "NODE_ENV=production rari start",
    "deploy:railway": "rari deploy railway",
    "deploy:render": "rari deploy render",
    "clean": "rm -rf dist",
    "lint": "oxlint && eslint",
    "lint:fix": "oxlint --fix && eslint --fix",
    "typecheck": "tsgo"
  },
  "dependencies": {
    "rari": "latest",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20260101.1",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "oxlint": "^1.36.0",
    "rolldown-vite": "^7.3.0",
    "tailwindcss": "^4.1.18"
  }
}
